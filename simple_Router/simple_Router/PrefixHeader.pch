#import "SRBaseController.h"
#import "UIViewController+PageRouter.h"


#define sr_StringIsEmpty(string) (string == nil  || !(string.length > 0) \
|| [[string stringByTrimmingCharactersInSet:[NSCharacterSet whitespaceCharacterSet]] length] == 0)
#define sr_StringIsNotEmpty(string) (string != nil  || string.length > 0 \
|| [[string stringByTrimmingCharactersInSet:[NSCharacterSet whitespaceCharacterSet]] length] > 0)


